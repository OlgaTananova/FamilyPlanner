{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost:5432;User Id={POSTGRES_USER};Password={POSTGRES_PASSWORD};Database={POSTGRES_DATABASE}"
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "${CORS_ALLOWED_ORIGINS}"
    ]
  },
  "AzureAdB2C": {
    "Instance": "{AZURE_AD_B2C_INSTANCE}",
    "ClientId": "{AZURE_AD_B2C_CLIENT_ID}",
    "Domain": "{AZURE_AD_B2C_DOMAIN}",
    "SignUpSignInPolicyId": "{AZURE_AD_B2C_SIGN_UP_SIGN_IN_POLICY_ID}",
    "Issuer": "{AZURE_AD_B2C_ISSUER}"
  }
}